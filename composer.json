{
  "name": "drakonli/php-imap",
  "description": "PHP OOP library for accessing mailbox by POP3/IMAP/NNTP using IMAP extension",
  "keywords": [
    "PHP",
    "IMAP",
    "mail"
  ],
  "homepage": "https://github.com/drakonli/php-imap",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Arthur Vinogradov",
      "homepage": "http://linkedin.com/in/drakonli",
      "email": "artur.drakonli@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0 <7.0.0",
    "ext-imap": "*",
    "ext-spl_types": "*",
    "drakonli/php-utils": "dev-master",
    "symfony/dependency-injection": "^3.0.0",
    "symfony/yaml": "^3.0.0",
    "symfony/config": "^3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*",
    "phpspec/prophecy": "~1.0",
    "php-mock/php-mock-phpunit": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "PhpImap\\": "src/PhpImap/"
    }
  },
  "minimum-stability": "dev"
}
